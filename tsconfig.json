{
  "compilerOptions": {
    "target": "esnext",
    "outDir": "./dist",
    "module": "commonjs",
    "lib": ["dom", "es2015"],
    "strict": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true
  }
}
